<h1>編集画面</h1>

<%= form_with model: @list do |f| %>
　
　<h4>料理画像</h4>
　<%= f.file_field :image %>
  <h4>レシピ名</h4>
  <%= f.text_field :name %>
  <h4>値段</h4>
  <%= f.text_field :price %>
  <h4>料理方法</h4>
  <%= f.text_area :explanation %>
  <h4>タグ名</h4>
  <%= f.text_field :category %>
  <%= f.submit '保存' %>
  
<% end %>